{
  "name": "subcare-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",      
    "db:push": "pnpm exec prisma db push --schema=packages/database/prisma/schema.prisma",
    "db:studio": "pnpm exec prisma studio --schema=packages/database/prisma/schema.prisma",
    "db:generate": "pnpm exec prisma generate --schema=packages/database/prisma/schema.prisma",
    "db:migrate": "pnpm exec prisma migrate dev --schema=packages/database/prisma/schema.prisma"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prisma": "5.22.0",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18"
  }
}
