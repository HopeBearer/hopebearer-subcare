{
  "name": "@subcare/database",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "db:generate": "pnpm exec prisma generate --schema=./prisma/schema.prisma DOTENV_CONFIG_PATH=../../.env",
    "db:push": "pnpm exec prisma db push --schema=./prisma/schema.prisma DOTENV_CONFIG_PATH=../../.env",
    "db:studio": "pnpm exec prisma studio --schema=./prisma/schema.prisma DOTENV_CONFIG_PATH=../../.env"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@tanstack/react-query": "^5.90.19",
    "react-intersection-observer": "^10.0.2"
  },
  "devDependencies": {
    "@subcare/eslint-config": "workspace:*",
    "@subcare/tsconfig": "workspace:*",
    "@types/node": "^20.11.19",
    "prisma": "5.22.0",
    "typescript": "^5.3.3"
  }
}
